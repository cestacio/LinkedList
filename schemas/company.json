{
    "$id": "http://example.com/example.json",
    "type": "object",
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "employees": {
            "$id": "/properties/employees",
            "type": "array"
        },
        "jobs": {
            "$id": "/properties/jobs",
            "type": "array"
        },
        "_id": {
            "$id": "/properties/_id",
            "type": "string",
            "title": "The _id Schema ",
            "default": "",
            "examples": [
                "5ac5726fde80c72a4ad86ad3"
            ]
        },
        "name": {
            "$id": "/properties/name",
            "type": "string",
            "title": "The Name Schema ",
            "default": "",
            "examples": [
                "Etsy"
            ]
        },
        "email": {
            "$id": "/properties/email",
            "type": "string",
            "title": "The Email Schema ",
            "default": "",
            "examples": [
                "recruiting@etsy.com"
            ]
        },
        "handle": {
            "$id": "/properties/handle",
            "type": "string",
            "title": "The Handle Schema ",
            "default": "",
            "examples": [
                "teametsy"
            ]
        },
        "password": {
            "$id": "/properties/password",
            "type": "string",
            "title": "The Password Schema ",
            "default": "",
            "examples": [
                "crafty"
            ]
        }
    }
}